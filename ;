
<!DOCTYPE html>
<html>
  <body onclick="main()">
    <canvas id="main_canvas" style="background-color:yellow"></canvas>
    <script src = "./jquery-1.11.2.js"></script>
    <script type = "text/javascript">
      Object.prototype.to_s = function(){
        $.map(this, function(k, v){
          return(k.toString + "-" + v.toString);
        }).join("_");
      }
      function main(){
        Style = function(){}
        Style.width = 1000;
        Style.height = 1000;
        Style.to_s();
        //$("#main_canvas").css(Style.to_s);
      }
    </script>
  </body>
</html>

